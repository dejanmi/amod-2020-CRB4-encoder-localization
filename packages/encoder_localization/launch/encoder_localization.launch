<launch>
  <arg name="veh"/>
  <arg name="pkg_name" value="encoder_localization"/>
  <arg name="node_name" default="encoder_localization_node"/>
  <group ns="$(arg veh)">
    <node pkg="$(arg pkg_name)" type="$(arg node_name).py" name="$(arg node_name)" output="screen">
    </node>
  </group>

</launch>
